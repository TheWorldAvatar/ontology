<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#"
     xml:base="http://www.theworldavatar.com/ontology/ontobim/ontoBIM"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="http://www.theworldavatar.com/ontology/ontobim/ontoBIM">
        <dc:date rdf:datatype="http://www.w3.org/2001/XMLSchema#string">07 September 2022</dc:date>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The Building Information Modelling ontology (OntoBIM) is an ontology developed to represent the detailed information and spatial relationships of buildings and their components (walls, doors, roofs, furniture, equipment) that is available in the Industry Foundation Classes (IFC) data schema. Information about their individual characteristics such as material, readings, and dimensions is combined from IFC or other data sources.</rdfs:comment>
        <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1</owl:versionInfo>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/elements/1.1/date -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/date"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.ontology-of-units-of-measure.org/resource/om-2/hasUnit -->

    <owl:ObjectProperty rdf:about="http://www.ontology-of-units-of-measure.org/resource/om-2/hasUnit">
        <rdfs:domain rdf:resource="http://www.ontology-of-units-of-measure.org/resource/om-2/Measure"/>
        <rdfs:range rdf:resource="http://www.ontology-of-units-of-measure.org/resource/om-2/Unit"/>
        <rdfs:isDefinedBy rdf:resource="http://www.ontology-of-units-of-measure.org/resource/om-2/"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ontology-of-units-of-measure.org/resource/om-2/hasValue -->

    <owl:ObjectProperty rdf:about="http://www.ontology-of-units-of-measure.org/resource/om-2/hasValue">
        <rdfs:domain rdf:resource="http://www.ontology-of-units-of-measure.org/resource/om-2/Quantity"/>
        <rdfs:range rdf:resource="http://www.ontology-of-units-of-measure.org/resource/om-2/Measure"/>
        <rdfs:isDefinedBy rdf:resource="http://www.ontology-of-units-of-measure.org/resource/om-2/"/>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasArea -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasArea">
        <rdfs:subPropertyOf rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasQuantity"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#Roof"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#RoofArea"/>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasAxisDirection -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasAxisDirection">
        <rdfs:subPropertyOf rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasDirection"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#LocalPlacement"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#DirectionVector"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship that determines the local placement&apos;s exact direction to the local Z axis.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasCompoundPlaneAngle -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasCompoundPlaneAngle">
        <rdfs:domain rdf:resource="https://w3id.org/bot#Site"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#CompoundPlaneAngle"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Links the Site to its world longitude or latitude. This relationship is not intended to be stated explicitly, but inferred from its sub-properties. For application, only the sub-properties are used.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasDirection -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasDirection">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#DirectionVector"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Links any entity to a direction vector. This relationship is not intended to be stated explicitly, but inferred from its sub-properties. For application, only the sub-properties are used.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasGeometricRepresentation -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasGeometricRepresentation">
        <rdfs:domain rdf:resource="https://w3id.org/bot#Element"/>
        <rdfs:range rdf:resource="https://standards.buildingsmart.org/IFC/DEV/IFC2x3/FINAL/OWL#IfcRepresentation"/>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasModelPosition -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasModelPosition">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/bot#Element"/>
        <rdfs:domain rdf:resource="https://w3id.org/bot#Zone"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#LocalPlacement"/>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasOrientation -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasOrientation">
        <rdfs:subPropertyOf rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasQuantity"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#Roof"/>
        <rdfs:range rdf:resource="http://www.ontology-of-units-of-measure.org/resource/om-2/Angle"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship between a roof and its angle/position relative to the Sun.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasPosition -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasPosition">
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#LocalPlacement"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#CartesianPoint"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship that defines the geometric position of a reference point.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasQuantity -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasQuantity">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="http://www.ontology-of-units-of-measure.org/resource/om-2/Quantity"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Links any entity to a specific quantity defined in the OM ontology. This relationship is not intended to be stated explicitly, but inferred from its sub-properties. For application, only the sub-properties are used.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasRefDirection -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasRefDirection">
        <rdfs:subPropertyOf rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasDirection"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#LocalPlacement"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#DirectionVector"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship that determines the local placement&apos;s direction to the local X axis.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasRefLatitude -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasRefLatitude">
        <rdfs:subPropertyOf rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasCompoundPlaneAngle"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/bot#Site"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#Latitude"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship between the site of a model and its world latitude.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasRefLongitude -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasRefLongitude">
        <rdfs:subPropertyOf rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasCompoundPlaneAngle"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/bot#Site"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#Longitude"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship between the site of a model and its world longtitude.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasRelativePositionTo -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasRelativePositionTo">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#LocalPlacement"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#LocalPlacement"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship between a component or spatial zone&apos;s placement position to their parent zone&apos;s placement position within the model.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasSlope -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasSlope">
        <rdfs:subPropertyOf rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasQuantity"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#Roof"/>
        <rdfs:range rdf:resource="http://www.ontology-of-units-of-measure.org/resource/om-2/Angle"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship between a roof and its slope angle.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontoubemmp/OntoUBEMMP.owl#consumesUtilities -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/ontoubemmp/OntoUBEMMP.owl#consumesUtilities">
        <rdfs:domain rdf:resource="https://w3id.org/bot#Zone"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/ontology/ontoubemmp/OntoUBEMMP.owl#UtilitiesConsumption"/>
        <rdfs:isDefinedBy rdf:resource="http://www.theworldavatar.com/ontology/ontoubemmp/OntoUBEMMP.owl#"/>
    </owl:ObjectProperty>
    


    <!-- https://github.com/cambridge-cares/TheWorldAvatar/blob/main/JPS_Ontology/ontology/ontotimeseries/OntoTimeSeries.owl#hasTimeSeries -->

    <owl:ObjectProperty rdf:about="https://github.com/cambridge-cares/TheWorldAvatar/blob/main/JPS_Ontology/ontology/ontotimeseries/OntoTimeSeries.owl#hasTimeSeries">
        <rdfs:domain rdf:resource="http://www.ontology-of-units-of-measure.org/resource/om-2/Measure"/>
        <rdfs:range rdf:resource="https://github.com/cambridge-cares/TheWorldAvatar/blob/main/JPS_Ontology/ontology/ontotimeseries/OntoTimeSeries.owl#TimeSeries"/>
        <rdfs:isDefinedBy rdf:resource="https://github.com/cambridge-cares/TheWorldAvatar/blob/main/JPS_Ontology/ontology/ontotimeseries/OntoTimeSeries.owl#"/>
    </owl:ObjectProperty>
    


    <!-- https://standards.buildingsmart.org/IFC/DEV/IFC2x3/FINAL/OWL#items_IfcRepresentation -->

    <owl:ObjectProperty rdf:about="https://standards.buildingsmart.org/IFC/DEV/IFC2x3/FINAL/OWL#items_IfcRepresentation">
        <rdfs:domain rdf:resource="https://standards.buildingsmart.org/IFC/DEV/IFC2x3/FINAL/OWL#IfcRepresentation"/>
        <rdfs:range rdf:resource="https://standards.buildingsmart.org/IFC/DEV/IFC2x3/FINAL/OWL#IfcRepresentationItem"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/bot#adjacentZone -->

    <owl:ObjectProperty rdf:about="https://w3id.org/bot#adjacentZone">
        <rdfs:domain rdf:resource="https://w3id.org/bot#Zone"/>
        <rdfs:range rdf:resource="https://w3id.org/bot#Zone"/>
        <owl:propertyDisjointWith rdf:resource="https://w3id.org/bot#intersectsZone"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/bot#"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/bot#containsZone -->

    <owl:ObjectProperty rdf:about="https://w3id.org/bot#containsZone">
        <rdfs:domain rdf:resource="https://w3id.org/bot#Zone"/>
        <rdfs:range rdf:resource="https://w3id.org/bot#Zone"/>
        <rdfs:comment rdf:resource="https://w3id.org/bot#"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/bot#hasBuilding -->

    <owl:ObjectProperty rdf:about="https://w3id.org/bot#hasBuilding">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/bot#containsZone"/>
        <rdfs:domain rdf:resource="https://w3id.org/bot#Site"/>
        <rdfs:range rdf:resource="https://w3id.org/bot#Building"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/bot#"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/bot#hasElement -->

    <owl:ObjectProperty rdf:about="https://w3id.org/bot#hasElement">
        <rdfs:domain rdf:resource="https://w3id.org/bot#Zone"/>
        <rdfs:range rdf:resource="https://w3id.org/bot#Element"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/bot#"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/bot#hasSpace -->

    <owl:ObjectProperty rdf:about="https://w3id.org/bot#hasSpace">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/bot#containsZone"/>
        <rdfs:domain rdf:resource="https://w3id.org/bot#Building"/>
        <rdfs:domain rdf:resource="https://w3id.org/bot#Storey"/>
        <rdfs:range rdf:resource="https://w3id.org/bot#Space"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/bot#"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/bot#hasStorey -->

    <owl:ObjectProperty rdf:about="https://w3id.org/bot#hasStorey">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/bot#containsZone"/>
        <rdfs:domain rdf:resource="https://w3id.org/bot#Building"/>
        <rdfs:range rdf:resource="https://w3id.org/bot#Storey"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/bot#"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/bot#hasSubElement -->

    <owl:ObjectProperty rdf:about="https://w3id.org/bot#hasSubElement">
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#Wall"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#Door"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#Window"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/bot#"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/bot#intersectsZone -->

    <owl:ObjectProperty rdf:about="https://w3id.org/bot#intersectsZone">
        <rdfs:domain rdf:resource="https://w3id.org/bot#Zone"/>
        <rdfs:range rdf:resource="https://w3id.org/bot#Zone"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/bot#"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.ontology-of-units-of-measure.org/resource/om-2/hasNumericalValue -->

    <owl:DatatypeProperty rdf:about="http://www.ontology-of-units-of-measure.org/resource/om-2/hasNumericalValue">
        <rdfs:domain rdf:resource="http://www.ontology-of-units-of-measure.org/resource/om-2/Measure"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
        <rdfs:isDefinedBy rdf:resource="http://www.ontology-of-units-of-measure.org/resource/om-2/"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasCoordinate -->

    <owl:DatatypeProperty rdf:about="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasCoordinate">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#CartesianPoint"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines the x-,y-, and z-coordinates for a Cartesian Point as a double value. This relationship is not intended to be stated explicitly, but inferred from its sub-properties. For application, only the sub-properties are used.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasDegree -->

    <owl:DatatypeProperty rdf:about="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasDegree">
        <rdfs:subPropertyOf rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasDegreeMinuteSecond"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#CompoundPlaneAngle"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship that defines the compound plane angle&apos;s degrees measure  as an integer value.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasDegreeMinuteSecond -->

    <owl:DatatypeProperty rdf:about="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasDegreeMinuteSecond">
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#CompoundPlaneAngle"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines the degree, minutes, seconds, and microseconds for a Compound Plane Angle as an integer value. This relationship is not intended to be stated explicitly, but inferred from its sub-properties. For application, only the sub-properties are used.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasDirectionRatio -->

    <owl:DatatypeProperty rdf:about="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasDirectionRatio">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#DirectionVector"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines the x,y, and z direction ratio for a Direction Vector as a double value. This relationship is not intended to be stated explicitly, but inferred from its sub-properties. For application, only the sub-properties are used.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasIfcId -->

    <owl:DatatypeProperty rdf:about="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasIfcId">
        <rdfs:domain rdf:resource="https://w3id.org/bot#Element"/>
        <rdfs:domain rdf:resource="https://w3id.org/bot#Zone"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasMicroSecond -->

    <owl:DatatypeProperty rdf:about="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasMicroSecond">
        <rdfs:subPropertyOf rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasDegreeMinuteSecond"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#CompoundPlaneAngle"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
