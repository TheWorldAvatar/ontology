    <!-- http://www.theworldavatar.com/ontology/ontobim/ontoBIM#ExtrudedAreaSolid -->

    <owl:Class rdf:about="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#ExtrudedAreaSolid">
