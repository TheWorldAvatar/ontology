<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#"
     xml:base="http://www.theworldavatar.com/ontology/ontobim/ontoBIM"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="http://www.theworldavatar.com/ontology/ontobim/ontoBIM">
        <dc:date rdf:datatype="http://www.w3.org/2001/XMLSchema#string">07 September 2022</dc:date>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The Building Information Modelling ontology (OntoBIM) is an ontology developed to represent the detailed information and spatial relationships of buildings and their components (walls, doors, roofs, furniture, equipment) that is available in the Industry Foundation Classes (IFC) data schema. Information about their individual characteristics such as material, readings, and dimensions is combined from IFC or other data sources.</rdfs:comment>
        <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1</owl:versionInfo>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/elements/1.1/date -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/date"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.ontology-of-units-of-measure.org/resource/om-2/hasUnit -->

    <owl:ObjectProperty rdf:about="http://www.ontology-of-units-of-measure.org/resource/om-2/hasUnit">
        <rdfs:domain rdf:resource="http://www.ontology-of-units-of-measure.org/resource/om-2/Measure"/>
        <rdfs:range rdf:resource="http://www.ontology-of-units-of-measure.org/resource/om-2/Unit"/>
        <rdfs:isDefinedBy rdf:resource="http://www.ontology-of-units-of-measure.org/resource/om-2/"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ontology-of-units-of-measure.org/resource/om-2/hasValue -->

    <owl:ObjectProperty rdf:about="http://www.ontology-of-units-of-measure.org/resource/om-2/hasValue">
        <rdfs:domain rdf:resource="http://www.ontology-of-units-of-measure.org/resource/om-2/Quantity"/>
        <rdfs:range rdf:resource="http://www.ontology-of-units-of-measure.org/resource/om-2/Measure"/>
        <rdfs:isDefinedBy rdf:resource="http://www.ontology-of-units-of-measure.org/resource/om-2/"/>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasArea -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasArea">
        <rdfs:subPropertyOf rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasQuantity"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#Roof"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#RoofArea"/>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasAxisDirection -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasAxisDirection">
        <rdfs:subPropertyOf rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasDirection"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#LocalPlacement"/>
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#PolygonalBoundedHalfSpace"/>
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#SurfacePlane"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#DirectionVector"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship that determines the local placement or geometry&apos;s exact direction to the local Z axis.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasBaseSurface -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasBaseSurface">
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#PolygonalBoundedHalfSpace"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#SurfacePlane"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship that defines the surface defining side for the half space.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasBooleanOperator -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasBooleanOperator">
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#BooleanClippingResult"/>
        <rdfs:range rdf:resource="https://standards.buildingsmart.org/IFC/DEV/IFC2x3/TC1/OWL#IfcBooleanOperator"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines the operator type for boolean operations.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasBounds -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasBounds">
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#Face"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#FaceOuterBound"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship that defines the boundaries of a face.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasCompoundPlaneAngle -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasCompoundPlaneAngle">
        <rdfs:domain rdf:resource="https://w3id.org/bot#Site"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#CompoundPlaneAngle"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Links the Site to its world longitude or latitude. This relationship is not intended to be stated explicitly, but inferred from its sub-properties. For application, only the sub-properties are used.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasConnectedFaces -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasConnectedFaces">
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#ClosedShell"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#Face"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship that defines the closed shell, comprising of a set of faces arcwise connected along common edges or vertices.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasContext -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasContext">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#Project"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#GeometricRepresentationContext"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship that references the project to their geometric representation context.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasDerivedAxis -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasDerivedAxis">
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#CartesianTransformationOperator"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#DirectionVector"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines a relationship from the operator to the derived axis direction, to get a list of mutually orthogonal, normalized vectors defining the transformation matrix in IFC. See IfcCartesianTransformationOperator3D and IfcCartesianTransformationOperator for more information.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasDerivedXAxis -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasDerivedXAxis">
        <rdfs:subPropertyOf rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasDerivedAxis"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines a relationship from the operator to the derived x-axis direction.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasDerivedYAxis -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasDerivedYAxis">
        <rdfs:subPropertyOf rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasDerivedAxis"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines a relationship from the operator to the derived y-axis direction.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasDirection -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasDirection">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#DirectionVector"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Links any entity to a direction vector. This relationship is not intended to be stated explicitly, but inferred from its sub-properties. For application, only the sub-properties are used.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasExteriorBoundary -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasExteriorBoundary">
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#FacetedBrep"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#ClosedShell"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship defining the exterior boundary of the solid through a shell. The shell normal shall point away from the interior of the solid.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasExtrusionDirection -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasExtrusionDirection">
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#ExtrudedAreaSolid"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#DirectionVector"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines the direction of the extrusion of the surface.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasExtrusionProfile -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasExtrusionProfile">
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#ExtrudedAreaSolid"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#RectangleProfileDefinition"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship defining the surface that is to be extruded, ie profile definition.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasFaceBoundary -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasFaceBoundary">
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#FaceOuterBound"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#PolyLoop"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship that defines the polyloop acting as the boundary for the face.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasFirstOperand -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasFirstOperand">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#BooleanClippingResult"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#GeometricRepresentation"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship that defines the first operand/geometry of the boolean clipping operation. This is usually a swept area solid or another boolean clipping result.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasGeometricRepresentation -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasGeometricRepresentation">
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#GeometricVoid"/>
        <rdfs:domain rdf:resource="https://w3id.org/bot#Element"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#ModelRepresentation3D"/>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasLocalOrigin -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasLocalOrigin">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#CartesianTransformationOperator"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#CartesianPoint"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines the required translation origin, specified as a cartesian point. The actual translation transform the geometric origin to the local origin</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasLocalPosition -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasLocalPosition">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#GeometricVoid"/>
        <rdfs:domain rdf:resource="https://w3id.org/bot#Element"/>
        <rdfs:domain rdf:resource="https://w3id.org/bot#Zone"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#LocalPlacement"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship between the abstract boundaries of a spatial zone or a component within the space or building and their placement position within the model. Note that this does not define their concrete geometric representation positions, which is only available in the ModelRepresentation3D class.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasNextVertex -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasNextVertex">
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#LineVertex"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#LineVertex"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship that linked the current vertex to the next vertex in a line.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasOrientation -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasOrientation">
        <rdfs:subPropertyOf rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasQuantity"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#Roof"/>
        <rdfs:range rdf:resource="http://www.ontology-of-units-of-measure.org/resource/om-2/Angle"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship between a roof and its angle/position relative to the Sun.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasParentContext -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasParentContext">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#GeometricRepresentationSubContext"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#GeometricRepresentationContext"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship between the parent and child context to share their contextual attributes.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasPolygonalBoundary -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasPolygonalBoundary">
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#PolygonalBoundedHalfSpace"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#Polyline"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship that defines the half space&apos;s 2D bounded curve, defined in the xy plane of the position coordinate system.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasProfileType -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasProfileType">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#RectangleProfileDefinition"/>
        <rdfs:range rdf:resource="http://standards.buildingsmart.org/IFC/DEV/IFC2x3/TC1/OWL#AREA"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship that defines that profile type of a profile definition of a surface.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasQuantity -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasQuantity">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="http://www.ontology-of-units-of-measure.org/resource/om-2/Quantity"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Links any entity to a specific quantity defined in the OM ontology. This relationship is not intended to be stated explicitly, but inferred from its sub-properties. For application, only the sub-properties are used.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasRefDirection -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasRefDirection">
        <rdfs:subPropertyOf rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasDirection"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#LocalPlacement"/>
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#PolygonalBoundedHalfSpace"/>
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#RectangleProfileDefinition"/>
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#SurfacePlane"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#DirectionVector"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship that determines the local placement or geometry direction to the local X axis.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasRefLatitude -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasRefLatitude">
        <rdfs:subPropertyOf rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasCompoundPlaneAngle"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/bot#Site"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#Latitude"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship between the site of a model and its world latitude.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasRefLongitude -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasRefLongitude">
        <rdfs:subPropertyOf rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#hasCompoundPlaneAngle"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/bot#Site"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/ontology/ontobim/ontoBIM#Longitude"/>
