        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/ontology/ontomop/OntoMOPs.owl</rdfs:isDefinedBy>
    </owl:Class>
    


