        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.theworldavatar.com/ontology/ontomops/OntoMOPs.owl#hasProvenance"/>
                <owl:allValuesFrom rdf:resource="http://www.theworldavatar.com/ontology/ontomops/OntoMOPs.owl#Provenance"/>
