        <rdfs:comment>Building concept as defined in gbXML</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.theworldavatar.com/ontology/ontobuilding/BuildingsLayer.owl#BuildingElement -->

    <owl:Class rdf:about="http://www.theworldavatar.com/ontology/ontobuilding/BuildingsLayer.owl#BuildingElement">
        <rdfs:subClassOf rdf:resource="http://www.theworldavatar.com/ontology/ontobuilding/BuildingsLayer.owl#BuildingSystem"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.theworldavatar.com/ontology/ontobuilding/BuildingsLayer.owl#isBuildingElementOf"/>
                <owl:someValuesFrom rdf:resource="http://www.theworldavatar.com/ontology/ontobuilding/BuildingsLayer.owl#Building"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Describes general concepts to model a building</rdfs:comment>
        <rdfs:comment>this class was known as &quot;BuildingPart&quot; in prior versions of the ontology</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.theworldavatar.com/ontology/ontobuilding/BuildingsLayer.owl#BuildingGroupEquipment -->

