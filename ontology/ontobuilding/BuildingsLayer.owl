        <rdfs:range rdf:resource="http://www.theworldavatar.com/ontology/ontobuilding/BuildingsLayer.owl#Building"/>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontobuilding/BuildingsLayer.owl#isEquipmentOf -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/ontobuilding/BuildingsLayer.owl#isEquipmentOf">
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/ontobuilding/BuildingsLayer.owl#Equipment"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.theworldavatar.com/ontology/ontobuilding/BuildingsLayer.owl#Building"/>
                    <rdf:Description rdf:about="http://www.theworldavatar.com/ontology/ontobuilding/BuildingsLayer.owl#Campus"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


