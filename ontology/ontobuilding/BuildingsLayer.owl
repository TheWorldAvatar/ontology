        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.theworldavatar.com/ontology/ontobuilding/BuildingsLayer.owl#Building"/>
                    <rdf:Description rdf:about="http://www.theworldavatar.com/ontology/ontobuilding/BuildingsLayer.owl#Equipment"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


