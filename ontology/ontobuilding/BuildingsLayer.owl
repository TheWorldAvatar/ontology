                <owl:someValuesFrom rdf:resource="http://www.theworldavatar.com/ontology/ontobuilding/BuildingsLayer.owl#Building"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Parameters describing the state and properties of a building</rdfs:comment>
    </owl:Class>
    


