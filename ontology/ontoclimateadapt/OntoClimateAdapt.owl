    <owl:Class rdf:about="https://github.com/TheWorldAvatar/ontoclimateadapt#Ecology"/>
    


    <!-- https://github.com/TheWorldAvatar/ontoclimateadapt#Economy -->


    <owl:Class rdf:about="https://github.com/TheWorldAvatar/ontoclimateadapt#Economy"/>
    


