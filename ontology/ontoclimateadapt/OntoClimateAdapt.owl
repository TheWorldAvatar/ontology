        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/2006/time"/>
        <rdfs:label>hasEnd</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://github.com/TheWorldAvatar/ontoclimateadapt#adaptedThrough -->


    <owl:ObjectProperty rdf:about="https://github.com/TheWorldAvatar/ontoclimateadapt#adaptedThrough">
        <rdfs:domain rdf:resource="https://github.com/TheWorldAvatar/ontoclimateadapt#ClimateChange"/>
        <rdfs:range rdf:resource="https://github.com/TheWorldAvatar/ontoclimateadapt#AdaptationMeasure"/>
        <rdfs:isDefinedBy rdf:resource="https://github.com/TheWorldAvatar/ontoclimateadapt"/>
        <rdfs:label>adaptedThrough</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://github.com/TheWorldAvatar/ontoclimateadapt#affects -->


    <owl:ObjectProperty rdf:about="https://github.com/TheWorldAvatar/ontoclimateadapt#affects">
        <rdfs:domain rdf:resource="https://github.com/TheWorldAvatar/ontoclimateadapt#ClimateChange"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://github.com/TheWorldAvatar/ontoclimateadapt#Ecology"/>
                    <rdf:Description rdf:about="https://github.com/TheWorldAvatar/ontoclimateadapt#Economy"/>
                    <rdf:Description rdf:about="https://github.com/TheWorldAvatar/ontoclimateadapt#HumanHealth"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment>An influence relationship between a phenomenon, process, or event and the systems, entities, or domains that experience its impact.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://github.com/TheWorldAvatar/ontoclimateadapt"/>
        <rdfs:label>affects</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://github.com/TheWorldAvatar/ontoclimateadapt#forAgeGroup -->


    <owl:ObjectProperty rdf:about="https://github.com/TheWorldAvatar/ontoclimateadapt#forAgeGroup">
        <rdfs:domain rdf:resource="https://github.com/TheWorldAvatar/ontoclimateadapt#AgeSpecificPopulationDensity"/>
        <rdfs:range rdf:resource="https://github.com/TheWorldAvatar/ontoclimateadapt#AgeGroup"/>
        <rdfs:comment>A relation indicating that a population density measurement applies to a specific age group defined by a minimum and maximum age.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://github.com/TheWorldAvatar/ontoclimateadapt"/>
        <rdfs:label>forAgeGroup</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://github.com/TheWorldAvatar/ontoclimateadapt#hasCause -->


    <owl:ObjectProperty rdf:about="https://github.com/TheWorldAvatar/ontoclimateadapt#hasCause">
        <rdfs:domain rdf:resource="https://github.com/TheWorldAvatar/ontoclimateadapt#ClimateChange"/>
        <rdfs:range rdf:resource="https://github.com/TheWorldAvatar/ontoclimateadapt#GreenhouseGasEmission"/>
        <rdfs:comment>A causal relationship between an effect (or phenomenon) and the process, or event responsible for its occurrence.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://github.com/TheWorldAvatar/ontoclimateadapt"/>
        <rdfs:label>hasCause</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://github.com/TheWorldAvatar/ontoclimateadapt#hasEffect -->


    <owl:ObjectProperty rdf:about="https://github.com/TheWorldAvatar/ontoclimateadapt#hasEffect">
        <rdfs:domain rdf:resource="https://github.com/TheWorldAvatar/ontoclimateadapt#ClimateChange"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://github.com/TheWorldAvatar/ontoclimateadapt#MeteorologicalExtremeEvent"/>
                    <rdf:Description rdf:about="https://github.com/TheWorldAvatar/ontoclimateadapt#OceanAcidification"/>
                    <rdf:Description rdf:about="https://github.com/TheWorldAvatar/ontoclimateadapt#SeaLevelRise"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment>A causal relationship between a phenomenon and the observable processes, events, or conditions that result from it.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://github.com/TheWorldAvatar/ontoclimateadapt"/>
        <rdfs:label>hasEffect</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://github.com/TheWorldAvatar/ontoclimateadapt#hasFrequency -->


    <owl:ObjectProperty rdf:about="https://github.com/TheWorldAvatar/ontoclimateadapt#hasFrequency">
        <rdfs:domain rdf:resource="https://github.com/TheWorldAvatar/ontoclimateadapt#NumberOfHeatwaveDays"/>
        <rdfs:range rdf:resource="https://github.com/TheWorldAvatar/ontoclimateadapt#TemporalFrequency"/>
        <rdfs:comment>An object property that links an observation to the temporal frequency at which the observed quantity is defined, measured, or aggregated.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://github.com/TheWorldAvatar/ontoclimateadapt"/>
        <rdfs:label>hasFrequency</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://github.com/TheWorldAvatar/ontoclimateadapt#hasNumberOfHeatwaveDays -->


    <owl:ObjectProperty rdf:about="https://github.com/TheWorldAvatar/ontoclimateadapt#hasNumberOfHeatwaveDays">
        <rdfs:domain rdf:resource="https://github.com/TheWorldAvatar/ontoclimateadapt#Location"/>
        <rdfs:range rdf:resource="https://github.com/TheWorldAvatar/ontoclimateadapt#NumberOfHeatwaveDays"/>
        <rdfs:comment>An object property that relates a location to an observation representing the number of days during which heatwave conditions occur.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://github.com/TheWorldAvatar/ontoclimateadapt"/>
        <rdfs:label>hasNumberOfHeatwaveDays</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://github.com/TheWorldAvatar/ontoclimateadapt#hasPopulationDensity -->


    <owl:ObjectProperty rdf:about="https://github.com/TheWorldAvatar/ontoclimateadapt#hasPopulationDensity">
        <rdfs:domain rdf:resource="https://github.com/TheWorldAvatar/ontoclimateadapt#Location"/>
        <rdfs:range rdf:resource="https://github.com/TheWorldAvatar/ontoclimateadapt#PopulationDensity"/>
        <rdfs:comment>Relates a Location to its corresponding PopulationDensity measurement.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://github.com/TheWorldAvatar/ontoclimateadapt"/>
        <rdfs:label>hasPopulationDensity</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://github.com/TheWorldAvatar/ontoclimateadapt#hasTemperature -->


    <owl:ObjectProperty rdf:about="https://github.com/TheWorldAvatar/ontoclimateadapt#hasTemperature">
        <rdfs:domain rdf:resource="https://github.com/TheWorldAvatar/ontoclimateadapt#Location"/>
        <rdfs:range rdf:resource="http://www.ontology-of-units-of-measure.org/resource/om-2/Temperature"/>
        <rdfs:comment>An object property that relates a location to a temperature quantity describing the thermal state of that location at a specific time.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://github.com/TheWorldAvatar/ontoclimateadapt"/>
        <rdfs:label>hasTemperature</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://github.com/TheWorldAvatar/ontoclimateadapt#hasTimeInterval -->


    <owl:ObjectProperty rdf:about="https://github.com/TheWorldAvatar/ontoclimateadapt#hasTimeInterval">
        <rdfs:domain rdf:resource="https://github.com/TheWorldAvatar/ontoclimateadapt#NumberOfHeatwaveDays"/>
        <rdfs:range rdf:resource="http://www.w3.org/2006/time#TemporalEntity"/>
        <rdfs:comment>An object property that relates an aggregated observation result to the temporal interval over which it was computed.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://github.com/TheWorldAvatar/ontoclimateadapt"/>
        <rdfs:label>hasTimeInterval</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://github.com/TheWorldAvatar/ontoclimateadapt#isDerivedFromEvent -->


    <owl:ObjectProperty rdf:about="https://github.com/TheWorldAvatar/ontoclimateadapt#isDerivedFromEvent">
        <rdfs:domain rdf:resource="https://github.com/TheWorldAvatar/ontoclimateadapt#NumberOfHeatwaveDays"/>
        <rdfs:range rdf:resource="https://github.com/TheWorldAvatar/ontoclimateadapt#Heatwave"/>
        <rdfs:comment>An object property that relates a derived quantitative indicator to one or more events from which the indicator has been computed or aggregated.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://github.com/TheWorldAvatar/ontoclimateadapt"/>
        <rdfs:label>isDerivedFromEvent</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://github.com/TheWorldAvatar/ontoclimateadapt#measuredBy -->


    <owl:ObjectProperty rdf:about="https://github.com/TheWorldAvatar/ontoclimateadapt#measuredBy">
        <rdfs:domain rdf:resource="https://github.com/TheWorldAvatar/ontoclimateadapt#ClimateChange"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://github.com/TheWorldAvatar/ontoclimateadapt#ClimateModel"/>
                    <rdf:Description rdf:about="https://github.com/TheWorldAvatar/ontoclimateadapt#MeteorologicalStation"/>
                    <rdf:Description rdf:about="https://github.com/TheWorldAvatar/ontoclimateadapt#SatelliteObservation"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment>The source of measurement, estimation, or observation through which evidence of climate change is obtained.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://github.com/TheWorldAvatar/ontoclimateadapt"/>
        <rdfs:label>measuredBy</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://github.com/TheWorldAvatar/ontoclimateadapt#occursAt -->


    <owl:ObjectProperty rdf:about="https://github.com/TheWorldAvatar/ontoclimateadapt#occursAt">
        <rdfs:domain rdf:resource="https://github.com/TheWorldAvatar/ontoclimateadapt#MeteorologicalExtremeEvent"/>
        <rdfs:range rdf:resource="https://github.com/TheWorldAvatar/ontoclimateadapt#Location"/>
        <rdfs:comment>A relation linking an event to the geographical location where it takes place or manifests its observable impacts.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://github.com/TheWorldAvatar/ontoclimateadapt"/>
        <rdfs:label>occursAt</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://github.com/TheWorldAvatar/ontoclimateadapt#studiedBy -->


    <owl:ObjectProperty rdf:about="https://github.com/TheWorldAvatar/ontoclimateadapt#studiedBy">
        <rdfs:domain rdf:resource="https://github.com/TheWorldAvatar/ontoclimateadapt#ClimateChange"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://github.com/TheWorldAvatar/ontoclimateadapt#AtmosphericScience"/>
                    <rdf:Description rdf:about="https://github.com/TheWorldAvatar/ontoclimateadapt#EnvironmentalScience"/>
                    <rdf:Description rdf:about="https://github.com/TheWorldAvatar/ontoclimateadapt#Paleoclimatology"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment>The relationship between a natural or environmental phenomenon and the scientific disciplines that systematically investigate it.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://github.com/TheWorldAvatar/ontoclimateadapt"/>
        <rdfs:label>studiedBy</rdfs:label>
    </owl:ObjectProperty>
    


