<?xml version="1.0"?>
<rdf:RDF xmlns="https://github.com/TheWorldAvatar/ontoclimateadapt#"
     xml:base="https://github.com/TheWorldAvatar/ontoclimateadapt"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:ontoclimateadapt="https://github.com/TheWorldAvatar/ontoclimateadapt#">
    <owl:Ontology rdf:about="https://github.com/TheWorldAvatar/ontoclimateadapt">
        <dc:date>26 January 2026</dc:date>
        <rdfs:comment>OntoClimateAdapt is an ontology designed to provide a formal semantic representation of data related to climate change adaptation research, initiatives, and actions. It enables the standardized description, integration, and interoperable exchange of information to support the development of resilient societies and adaptive capacities in the face of climate change.</rdfs:comment>
        <owl:versionInfo>1</owl:versionInfo>
        <gitCommitHash>c0599beca8df55873a1ab061dee64e52c510c6a0</gitCommitHash>
    </owl:Ontology>
    


