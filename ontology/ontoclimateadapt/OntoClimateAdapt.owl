    <owl:Class rdf:about="http://www.w3.org/2006/time#TemporalEntity"/>
    


    <!-- https://github.com/TheWorldAvatar/ontoclimateadapt#AdaptationMeasure -->


    <owl:Class rdf:about="https://github.com/TheWorldAvatar/ontoclimateadapt#AdaptationMeasure">
        <rdfs:comment>A deliberate action, strategy, or intervention implemented to reduce the adverse impacts of climate change</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://github.com/TheWorldAvatar/ontoclimateadapt"/>
        <rdfs:label>AdaptationMeasure</rdfs:label>
    </owl:Class>
    


    <!-- https://github.com/TheWorldAvatar/ontoclimateadapt#AgeGroup -->


    <owl:Class rdf:about="https://github.com/TheWorldAvatar/ontoclimateadapt#AgeGroup">
        <rdfs:comment>A group of people whose ages fall within a specified range.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://github.com/TheWorldAvatar/ontoclimateadapt"/>
        <rdfs:label>AgeGroup</rdfs:label>
    </owl:Class>
    


    <!-- https://github.com/TheWorldAvatar/ontoclimateadapt#AgeSpecificPopulationDensity -->


    <owl:Class rdf:about="https://github.com/TheWorldAvatar/ontoclimateadapt#AgeSpecificPopulationDensity">
        <rdfs:subClassOf rdf:resource="https://github.com/TheWorldAvatar/ontoclimateadapt#PopulationDensity"/>
        <rdfs:isDefinedBy rdf:resource="https://github.com/TheWorldAvatar/ontoclimateadapt"/>
        <rdfs:label>AgeSpecificPopulationDensity</rdfs:label>
    </owl:Class>
    


    <!-- https://github.com/TheWorldAvatar/ontoclimateadapt#AnnualFrequency -->


    <owl:Class rdf:about="https://github.com/TheWorldAvatar/ontoclimateadapt#AnnualFrequency">
        <rdfs:subClassOf rdf:resource="https://github.com/TheWorldAvatar/ontoclimateadapt#TemporalFrequency"/>
        <rdfs:comment>A temporal frequency representing the count of occurrences within a calendar year.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://github.com/TheWorldAvatar/ontoclimateadapt"/>
        <rdfs:label>AnnualFrequency</rdfs:label>
    </owl:Class>
    


    <!-- https://github.com/TheWorldAvatar/ontoclimateadapt#AtmosphericScience -->


    <owl:Class rdf:about="https://github.com/TheWorldAvatar/ontoclimateadapt#AtmosphericScience"/>
    


    <!-- https://github.com/TheWorldAvatar/ontoclimateadapt#ClimateAdaptationAction -->


    <owl:Class rdf:about="https://github.com/TheWorldAvatar/ontoclimateadapt#ClimateAdaptationAction">
        <rdfs:subClassOf rdf:resource="https://github.com/TheWorldAvatar/ontoclimateadapt#AdaptationMeasure"/>
        <rdfs:comment>A concrete activity or measure undertaken to reduce the impacts of climate change or increase the resilience of human or natural systems.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://github.com/TheWorldAvatar/ontoclimateadapt"/>
