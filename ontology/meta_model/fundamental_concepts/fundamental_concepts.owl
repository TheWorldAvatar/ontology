    <!-- http://www.theworldavatar.com/ontology/meta_model/fundamental_concepts/fundamental_concepts.owl#hasOrigin -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/meta_model/fundamental_concepts/fundamental_concepts.owl#hasOrigin">
        <rdfs:subPropertyOf rdf:resource="http://www.theworldavatar.com/ontology/meta_model/fundamental_concepts/fundamental_concepts.owl#involvesObject"/>
        <owl:inverseOf rdf:resource="http://www.theworldavatar.com/ontology/meta_model/fundamental_concepts/fundamental_concepts.owl#isOriginOf"/>
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/meta_model/fundamental_concepts/fundamental_concepts.owl#DirectedN-aryRelation"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The relation identifies the Object that is the origin of a DirectedN-aryRelation</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/meta_model/fundamental_concepts/fundamental_concepts.owl#hasTarget -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/meta_model/fundamental_concepts/fundamental_concepts.owl#hasTarget">
        <rdfs:subPropertyOf rdf:resource="http://www.theworldavatar.com/ontology/meta_model/fundamental_concepts/fundamental_concepts.owl#involves"/>
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/meta_model/fundamental_concepts/fundamental_concepts.owl#DirectedN-aryRelation"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The relation hasTarget identifies the Objects or feature values (i.e., instances of FeatureSpace) that are the targets of a DirectedN-aryRelation.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/meta_model/fundamental_concepts/fundamental_concepts.owl#hasTargetObject -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/meta_model/fundamental_concepts/fundamental_concepts.owl#hasTargetObject">
        <rdfs:subPropertyOf rdf:resource="http://www.theworldavatar.com/ontology/meta_model/fundamental_concepts/fundamental_concepts.owl#hasTarget"/>
        <rdfs:subPropertyOf rdf:resource="http://www.theworldavatar.com/ontology/meta_model/fundamental_concepts/fundamental_concepts.owl#involvesObject"/>
        <owl:inverseOf rdf:resource="http://www.theworldavatar.com/ontology/meta_model/fundamental_concepts/fundamental_concepts.owl#isTargetOf"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/ontology/meta_model/fundamental_concepts/fundamental_concepts.owl#Object"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The relation identifies the Objects or feature values (i.e., instances of FeatureSpace) that are the targets of a DirectedN-aryRelation.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/meta_model/fundamental_concepts/fundamental_concepts.owl#inter-objectRelation -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/meta_model/fundamental_concepts/fundamental_concepts.owl#inter-objectRelation">
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/meta_model/fundamental_concepts/fundamental_concepts.owl#Object"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/ontology/meta_model/fundamental_concepts/fundamental_concepts.owl#Object"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">subsumes all types of binary relations between Objects</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/meta_model/fundamental_concepts/fundamental_concepts.owl#involves -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/meta_model/fundamental_concepts/fundamental_concepts.owl#involves">
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/meta_model/fundamental_concepts/fundamental_concepts.owl#RelationClass"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.theworldavatar.com/ontology/meta_model/fundamental_concepts/fundamental_concepts.owl#FeatureSpace"/>
                    <rdf:Description rdf:about="http://www.theworldavatar.com/ontology/meta_model/fundamental_concepts/fundamental_concepts.owl#Object"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">identifies the Objects or feature values (i.e., instnace of FeatureSpace) that are involved in an n-ary relation</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/meta_model/fundamental_concepts/fundamental_concepts.owl#involvesObject -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/meta_model/fundamental_concepts/fundamental_concepts.owl#involvesObject">
        <rdfs:subPropertyOf rdf:resource="http://www.theworldavatar.com/ontology/meta_model/fundamental_concepts/fundamental_concepts.owl#involves"/>
        <owl:inverseOf rdf:resource="http://www.theworldavatar.com/ontology/meta_model/fundamental_concepts/fundamental_concepts.owl#isInvolvedInN-aryRelation"/>
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/meta_model/fundamental_concepts/fundamental_concepts.owl#RelationClass"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/ontology/meta_model/fundamental_concepts/fundamental_concepts.owl#Object"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">identifies the Objects involved in an n-ary relation</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/meta_model/fundamental_concepts/fundamental_concepts.owl#isInvolvedInN-aryRelation -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/meta_model/fundamental_concepts/fundamental_concepts.owl#isInvolvedInN-aryRelation">
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/meta_model/fundamental_concepts/fundamental_concepts.owl#Object"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/ontology/meta_model/fundamental_concepts/fundamental_concepts.owl#RelationClass"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">denotes the relation between an Object and a RelationClass</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/meta_model/fundamental_concepts/fundamental_concepts.owl#isOfType -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/meta_model/fundamental_concepts/fundamental_concepts.owl#isOfType">
        <rdfs:subPropertyOf rdf:resource="http://www.theworldavatar.com/ontology/meta_model/fundamental_concepts/fundamental_concepts.owl#object-featureRelation"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The relation isOfType assigns value types to Objects. Based on this characteristics, a reasoner can infer if an Object belongs to a predefined ontology view.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/meta_model/fundamental_concepts/fundamental_concepts.owl#isOriginOf -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/meta_model/fundamental_concepts/fundamental_concepts.owl#isOriginOf">
        <rdfs:subPropertyOf rdf:resource="http://www.theworldavatar.com/ontology/meta_model/fundamental_concepts/fundamental_concepts.owl#isInvolvedInN-aryRelation"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/ontology/meta_model/fundamental_concepts/fundamental_concepts.owl#DirectedN-aryRelation"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">denotes the relation between the originating Object and a DirectedN-aryRelation</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/meta_model/fundamental_concepts/fundamental_concepts.owl#isTargetOf -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/meta_model/fundamental_concepts/fundamental_concepts.owl#isTargetOf">
        <rdfs:subPropertyOf rdf:resource="http://www.theworldavatar.com/ontology/meta_model/fundamental_concepts/fundamental_concepts.owl#isInvolvedInN-aryRelation"/>
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/meta_model/fundamental_concepts/fundamental_concepts.owl#Object"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">denotes the relation between the target Object and a DirectedN-aryRelation</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/meta_model/fundamental_concepts/fundamental_concepts.owl#object-featureRelation -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/meta_model/fundamental_concepts/fundamental_concepts.owl#object-featureRelation">
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/meta_model/fundamental_concepts/fundamental_concepts.owl#Object"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/ontology/meta_model/fundamental_concepts/fundamental_concepts.owl#FeatureSpace"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The relation object-featureRelation denotes the relation between an Object and its feature values (i.e., instances of FeatureSpace).</rdfs:comment>
    </owl:ObjectProperty>
    


