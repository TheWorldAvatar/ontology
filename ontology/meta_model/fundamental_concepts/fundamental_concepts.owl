    <!-- http://www.theworldavatar.com/ontology/meta_model/fundamental_concepts/fundamental_concepts.owl#relationAttribute -->

    <owl:DatatypeProperty rdf:about="http://www.theworldavatar.com/ontology/meta_model/fundamental_concepts/fundamental_concepts.owl#relationAttribute">
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/meta_model/fundamental_concepts/fundamental_concepts.owl#RelationClass"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">relationAttribute identifies the datatype value that characterizes a relation class</rdfs:comment>
    </owl:DatatypeProperty>
    


